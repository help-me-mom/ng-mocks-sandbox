{
  "name": "ng-mocks-sandbox",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.0.5",
    "@angular/common": "11.0.5",
    "@angular/compiler": "11.0.5",
    "@angular/core": "11.0.5",
    "@angular/forms": "11.0.5",
    "@angular/platform-browser": "11.0.5",
    "@angular/platform-browser-dynamic": "11.0.5",
    "@angular/router": "11.0.5",
    "@types/jasmine": "3.6.2",
    "core-js": "3.8.1",
    "jasmine-core": "3.6.0",
    "ng-mocks": "11.2.5",
    "rxjs": "6.5.5",
    "tslib": "2.0.3",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "typescript": "4.0.5"
  },
  "keywords": [
    "angular",
    "ng-mocks",
    "mock",
    "mocking",
    "test",
    "testing",
    "sandbox",
    "playground"
  ],
  "description": "A sandbox with guides and examples of tests to play with ng-mocks",
  "renovate": {
    "extends": ["config:base", ":semanticCommitTypeAll(chore)"],
    "rangeStrategy": "pin",
    "rebaseWhen": "conflicted",
    "separateMajorMinor": true,
    "separateMinorPatch": true,
    "separateMultipleMajor": true,
    "ignoreDeprecated": false,
    "semanticCommitScope": "{{#if baseDir}}{{baseDir}}{{else}}deps{{/if}}",
    "additionalBranchPrefix": "{{#if baseDir}}{{baseDir}}/{{else}}root/{{/if}}",
    "reviewers": [
      "satanTime"
    ],
    "packageRules": [
      {
        "packagePatterns": [
          "^@angular/"
        ],
        "allowedVersions": "<=11"
      },
      {
        "packageNames": [
          "core-js"
        ],
        "allowedVersions": "<=3.8"
      },
      {
        "packageNames": [
          "rxjs"
        ],
        "allowedVersions": "<=6.5"
      },
      {
        "packageNames": [
          "tslib"
        ],
        "allowedVersions": "<=2"
      },
      {
        "packageNames": [
          "zone.js"
        ],
        "allowedVersions": "<=0.10"
      },
      {
        "packageNames": [
          "@types/jasmine",
          "jasmine-core"
        ],
        "allowedVersions": "<=3.6"
      },
      {
        "packageNames": [
          "typescript"
        ],
        "allowedVersions": "<=4.0"
      }
    ]
  }
}
