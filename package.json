{
  "name": "ng-mocks-sandbox",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.0.3",
    "@angular/common": "11.0.3",
    "@angular/compiler": "11.0.3",
    "@angular/core": "11.0.3",
    "@angular/forms": "11.0.3",
    "@angular/platform-browser": "11.0.3",
    "@angular/platform-browser-dynamic": "11.0.3",
    "@angular/router": "11.0.3",
    "@types/jasmine": "3.6.2",
    "core-js": "3.8.1",
    "jasmine-core": "3.6.0",
    "ng-mocks": "11.2.3",
    "rxjs": "6.6.3",
    "tslib": "2.0.3",
    "zone.js": "0.11.3"
  },
  "devDependencies": {
    "typescript": "4.1.3"
  },
  "keywords": [
    "angular",
    "ng-mocks",
    "mock",
    "mocking",
    "test",
    "testing",
    "sandbox",
    "playground"
  ],
  "description": "A sandbox with guides and examples of tests to play with ng-mocks",
  "renovate": {
    "rangeStrategy": "pin",
    "rebaseWhen": "conflicted",
    "separateMajorMinor": true,
    "separateMinorPatch": true,
    "separateMultipleMajor": true
  }
}
