{
  "name": "ng-mocks-sandbox",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "description": "A sandbox with guides and examples to play with ng-mocks",
  "keywords": [
    "angular",
    "ng-mocks",
    "mock",
    "mocking",
    "test",
    "testing",
    "sandbox",
    "playground"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "prettier:repo": "prettier --loglevel warn -u -w .",
    "prettier:check": "prettier -u -c .",
    "ts:check": "tsc --noEmit",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@angular/animations": "12.2.1",
    "@angular/common": "12.2.1",
    "@angular/compiler": "12.2.1",
    "@angular/core": "12.2.1",
    "@angular/forms": "12.2.1",
    "@angular/platform-browser": "12.2.1",
    "@angular/platform-browser-dynamic": "12.2.1",
    "@angular/router": "12.2.1",
    "@types/jasmine": "3.8.2",
    "core-js": "3.11.1",
    "jasmine-core": "3.8.0",
    "ng-mocks": "12.4.0",
    "rxjs": "6.6.7",
    "tslib": "2.3.1",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.1",
    "@angular/cli": "12.2.1",
    "@angular/compiler-cli": "12.2.1",
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@types/node": "14.17.7",
    "husky": "7.0.1",
    "karma": "5.2.3",
    "karma-chrome-launcher": "3.1.0",
    "karma-ie-launcher": "1.0.0",
    "karma-jasmine": "4.0.1",
    "karma-junit-reporter": "2.0.1",
    "prettier": "2.3.2",
    "puppeteer": "10.2.0",
    "ts-node": "10.1.0",
    "typescript": "4.3.5"
  },
  "renovate": {
    "extends": [
      "config:base",
      ":semanticCommitTypeAll(chore)"
    ],
    "rangeStrategy": "pin",
    "rebaseWhen": "conflicted",
    "separateMajorMinor": true,
    "separateMinorPatch": true,
    "separateMultipleMajor": true,
    "ignoreDeprecated": false,
    "semanticCommitScope": "{{#if baseDir}}{{baseDir}}{{else}}deps{{/if}}",
    "additionalBranchPrefix": "{{#if baseDir}}{{baseDir}}/{{else}}root/{{/if}}",
    "reviewers": [
      "satanTime"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "IE 11",
    "not IE 10"
  ]
}
