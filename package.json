{
  "name": "ng-mocks-sandbox",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "description": "A sandbox with guides and examples to play with ng-mocks",
  "keywords": [
    "angular",
    "ng-mocks",
    "mock",
    "mocking",
    "test",
    "testing",
    "sandbox",
    "playground"
  ],
  "scripts": {
    "npm": "npm",
    "ng": "ng",
    "start": "ng test",
    "serve": "ng serve --disable-host-check",
    "test": "ng test",
    "prettier:repo": "prettier --loglevel warn -u -w .",
    "prettier:check": "prettier -u -c .",
    "ts:check": "tsc --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
    "@angular/animations": "19.2.17",
    "@angular/common": "19.2.17",
    "@angular/compiler": "19.2.17",
    "@angular/core": "19.2.17",
    "@angular/forms": "19.2.17",
    "@angular/platform-browser": "19.2.17",
    "@angular/platform-browser-dynamic": "19.2.17",
    "@angular/router": "19.2.17",
    "@types/jasmine": "5.1.13",
    "core-js": "3.47.0",
    "jasmine-core": "5.13.0",
    "ng-mocks": "14.14.0",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.18",
    "@angular/cli": "19.2.18",
    "@angular/compiler-cli": "19.2.17",
    "@commitlint/cli": "20.2.0",
    "@commitlint/config-conventional": "20.2.0",
    "@types/jasmine": "5.1.13",
    "@types/node": "24.10.4",
    "husky": "9.1.7",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-ie-launcher": "1.0.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "karma-junit-reporter": "2.0.1",
    "npm": "11.6.2",
    "prettier": "3.7.4",
    "puppeteer": "24.33.1",
    "ts-node": "10.9.2",
    "typescript": "5.8.3"
  },
  "engines": {
    "npm": "11.6.2"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
